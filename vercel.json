{
  "rewrites": [{ "source": "/(.*)", "destination": "/" }],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/src/main.jsx",
      "methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS"],
      "headers": {
        "Access-Control-Allow-Origin": "https://type-blog-backend.vercel.app",
        "Access-Control-Allow-Credentials": "true"
      }
    }
  ]
}
